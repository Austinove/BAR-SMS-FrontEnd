    "scripts": {
        "start": "react-scripts start",
        "dev": "npm-run-all -p watch-sass start-js",
        "start-js": "react-scripts start",
        "build": "npm run minify-css && react-scripts build",
        "minify-css": "node-sass --output-style src/assets/scss/style.scss src/assets/scss/style.css",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "watch-sass": "sass --watch src/assets/scss/style.scss:src/assets/scss/style.css"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
